<div id="admin-only">
	<h2>Upgrade a User</h2>
	<input id="username-input" type="text" placeholder="Username" />
	<button type="button" id="check-credentials" class="btn btn-default">Check User Credentials</button>
	<p id="credentials"></p>
	<select id="user-type">
		<option value="sexpert" selected>Sexpert</option>
		<option value="employee">Employee</option>
		<option value="admin">Admin</option>
	</select>
	<button type="button" id="submit-type" class="btn btn-default">Upgrade</button>
	<p>Sexpert Profile</p>
	<input id="experience" type="number" placeholder="years" style="width: 4em;" /> Experience <br/>
	<textarea id="bio" placeholder="Bio" style="height: 8em; width: 24em;"></textarea><br/>
	<button id="submit-profile" clas="btn btn-default">Submit Profile</button>
	<p id="message"></p>
</div>